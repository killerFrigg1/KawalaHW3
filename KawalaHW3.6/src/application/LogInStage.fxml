<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="200.0" prefWidth="300.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.LogInStageController">
	<bottom>
		<HBox alignment="CENTER" BorderPane.alignment="CENTER">
			<children>
				<Button fx:id="logInButton" mnemonicParsing="false" onAction="#logIn"
					text="Zaloguj" />
				<Button fx:id="cancelButton" mnemonicParsing="false" text="Anuluj" />
			</children>
		</HBox>
	</bottom>
	<center>
		<VBox alignment="CENTER" BorderPane.alignment="CENTER">
			<children>
				<TextField fx:id="username" promptText="UÅ¼ytkownik" />
				<PasswordField fx:id="password" promptText="Password" />
			</children>
		</VBox>
	</center>
</BorderPane>
